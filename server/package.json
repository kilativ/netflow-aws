{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "main": "src/server.ts",
  "files": [
    "dist/server/src/**/*.*",
    "dist/shared/**/*.*",
    "package.json"
  ],
  "scripts": {
    "build": "npm run webpack",
    "build-ts": "tsc",
    "claudia-prep": "claudia generate-serverless-express-proxy --express-module dist/server/src/server-aws",
    "claudia-create": "claudia create --handler dist/server/src/lambda.handler --deploy-proxy-api --region us-east-1 --set-env-from-json lambda-env.json",
    "claudia-update": "claudia update --handler dist/server/src/lambda.handler --deploy-proxy-api --region us-east-1 --set-env-from-json lambda-env.json",
    "run-jobs": "ts-node-transpile-only ./src/jobs.ts",
    "run-server-quick": "ts-node-transpile-only ./src/server.ts",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "npm run serve",
    "watch": "npm run build-ts && npm run watch-all",
    "watch-all": "concurrently -k -p \"[{name}]\" -n \"TSC,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon dist/server/src/server.js",
    "watch-ts": "tsc -w",
    "webpack": "webpack"
  },
  "dependencies": {
    "aws-sdk": "^2.804.0",
    "aws-serverless-express": "^3.4.0",
    "body-parser": "^1.19.0",
    "copy-dynamodb-table": "^2.0.20",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-session": "^1.17.1",
    "google-auth-library": "^6.1.3",
    "moment": "^2.29.0",
    "plaid": "^7.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.11.2",
    "@types/shelljs": "^0.8.8",
    "@types/uuid": "^8.3.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "shelljs": "^0.8.4",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}
