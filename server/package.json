{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "build": "npm run webpack && npm run copy-assets",
    "build-ts": "tsc",
    "run-jobs": "ts-node-transpile-only ./src/jobs.ts",
    "copy-assets": "ts-node copyAssets.ts",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "npm run serve",
    "watch": "npm run build-ts && npm run copy-assets && npm run watch-all",
    "watch-all": "concurrently -k -p \"[{name}]\" -n \"TSC,Assets,Node\" -c \"cyan.bold,yellow.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-assets\" \"npm run watch-node\"",
    "watch-assets": "nodemon --watch src/views -e html,css --exec npm run copy-assets",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "webpack": "webpack"
  },
  "dependencies": {
    "aws-sdk": "^2.804.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "googleapis": "^66.0.0",
    "moment": "^2.29.0",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "plaid": "^7.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.11.2",
    "@types/shelljs": "^0.8.8",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "shelljs": "^0.8.4",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}
